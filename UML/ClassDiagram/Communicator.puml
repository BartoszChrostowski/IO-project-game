@startuml
skinparam shadowing false
skinparam monochrome true
skinparam classAttributeIconSize 0
hide circles

skinparam class {
BackgroundColor white
}

class Communicator {
- gmSocket: Socket
- agentSocket: Socket

+ SendMessageToAgent()
+ SendMessageToGM()
}

class AgentMessage {
+ payload: string
}

class GMMessage {
+ id: int
+ payload: string
}
@enduml